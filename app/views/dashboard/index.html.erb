<h1>Dashboard</h1>
<table class="level_of_interest">
 <th>Interest Level</th><th>Count</th>
<% @count_by_interest.each do |level, count| %>
  <tr>
    <td><%= level %></td><td><%= count %></td>
  </tr>     
<% end %>
</table>
<ul class="top_3">
  <% @top_3.each do |company| %>
    <li><%= company.name %>: <%= company.average_level_of_interest %></li>
  <% end %>
</ul>
<ul class="by_location">
   <% @count_by_location.each do |city, count|  %>
     <li><%= link_to city, "#{companies_path}/?location=#{city}" %>: <%= count %></li>
   <% end %>
</ul>

