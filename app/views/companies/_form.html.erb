<div class="row well">
  <%= form_for @company do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :city %>
      <%= f.text_field :city, class: "form-control" %>
    </div>

    <%= f.submit class: "btn btn-success"%>
  <% end %>
</div>
